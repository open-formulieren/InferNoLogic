    "test": "jest",
    "format": "prettier --write 'src/**/*'",
    "checkformat": "prettier --check 'src/**/*'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/open-formulieren/InferNoLogic.git"
  },
