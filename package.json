  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.7",
    "@types/node": "^18",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsd": "^0.28.1",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
